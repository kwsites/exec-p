{
  "name": "@kwsites/exec-p",
  "version": "0.2.0",
  "description": "Execute a command and retrieve data as a promise.",
  "main": "./dist/",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "repository": "git://github.com/kwsites/exec-p.git",
  "scripts": {
    "clean": "rimraf ./dist",
    "definitions": "dts-generator --name kwsites__exec-p --project ./ --out ./dist/index.d.ts",
    "generate": "tsc && npm run definitions",
    "preversion": "npm run clean && npm run generate",
    "postversion": "npm publish && git push && git push --tags && npm run clean",
    "typings:clean": "rimraf ./dist/typings",
    "typings:build": "tsc -p ./tsconfig.types.json"
  },
  "keywords": [
    "child process",
    "exec",
    "spawn"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.7.1",
    "dts-generator": "^2.1.0",
    "rimraf": "^2.6.2",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {}
}
